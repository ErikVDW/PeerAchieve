<body>


<!--NAVBAR-->
    <div class="navbar navbar-inverse navbar-fixed-top" role="navigation"><!--creates the dark grey navbar at the top-->
      <div class="container"><!--moves the content to the middle-->
        <!-- MOBILE MENU -->
        <div class="navbar-header"><!--for displaying the mobile menu-->
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="/"></a>
        </div>
        <!-- DESKTOP MENU -->
        <nav class="navbar-collapse collapse"><!--holds the menu items-->
          <ul class="nav navbar-nav">
            <li role="presentation" class="<%= 'active' if current_page?(root_path) %>"><%= link_to root_path do %>Achieve<span style="font-family: 'Lobster', cursive;">More</span><% end %></li>
            <!-- <li><a href="#description">Description</a></li>
            <li><a href="#team">Team</a></li> -->
            <!--<li role="presentation" class="<%# 'active' if current_page?(blogs_path) %>"><a href="blog">Blog</a></li>-->
            <li role="presentation" class="<%= 'active' if current_page?(about_path) %>"><a href="about"><span class="glyphicon glyphicon-info-sign"></span> About</a></li>
            <li role="presentation" class="<%= 'active' if current_page?(contact_path) %>"><a href="contact"><span class="glyphicon glyphicon-earphone"></span> Contact</a></li>
            <li role="presentation" class="<%= 'active' if current_page?(faq_path) %>"><a href="faq"><span class="glyphicon glyphicon-question-sign"></span> FAQ</a></li>
          </ul>
          
          <div class="navbar-form navbar-right">
          <%= form_for(resource, as: resource_name, url: session_path(resource_name), html: { style: 'display:inline' } ) do |f| %>
            <% if user_signed_in? %>
              <p style="color:white">You are logged in as <%= current_user.email %>              
              <%= link_to 'log out', destroy_user_session_path, method: :delete %></p>
              
              <% if current_user.is_admin? %>
                <%= link_to 'admin panel', rails_admin_path %>
              <% end %>
              
            <% else %>
                <div class="form-group">
                  <%= f.email_field :email, autofocus: true, placeholder: 'Email', class: 'form-control' %>
                </div>
                <div class="form-group">
                  <%= f.password_field :password, autocomplete: 'off', placeholder: 'Password', class: 'form-control' %>
                </div>
                <%= f.submit 'Sign In', class: 'btn btn-primary' %><!--<span style="color: #a09797"> or </span>-->
                <!--<li><%# link_to "Sign In", new_user_session_path %></li>-->
            <% end %>
          <% end %>
          <!--<div class="form-group">-->
          <!--  <% button_to "Sign Up", new_user_registration_path, class: 'btn btn-success', method: :get %>-->
          <!--</div>-->
        </div>  
        </nav><!--/menu items-->
      </div><!--/container-->
    </div><!--/NAVBAR-->