    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>

    <!-- For stellar.js -->
    <script type="text/javascript">$(function(){ $(window).stellar(); });</script><!--Does need to be called on window-->
    
    <!--Script for smooth scrolling to DESCRIPTION and TEAM sections-->
    <script type="text/javascript">
      var $root = $('html, body');
        $('.navbar-nav a').click(function() {
            var href = $.attr(this, 'href');
            $root.animate({
                scrollTop: $(href).offset().top
            }, 500, function () {
                window.location.hash = href;
            });
            return false;
        });
    </script>

